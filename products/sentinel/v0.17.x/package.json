{
  "name": "sentinel-docs",
  "description": "Documentation website for HashiCorp Sentinel",
  "version": "1.0.0",
  "author": "HashiCorp",
  "dependencies": {
    "@hashicorp/mktg-global-styles": "2.1.0",
    "@hashicorp/nextjs-scripts": "14.1.1",
    "@hashicorp/react-button": "4.0.0",
    "@hashicorp/react-checkbox-input": "3.0.4",
    "@hashicorp/react-content": "6.2.0",
    "@hashicorp/react-docs-page": "10.4.0",
    "@hashicorp/react-docs-sidenav": "6.0.1",
    "@hashicorp/react-footer": "3.3.4",
    "@hashicorp/react-global-styles": "4.6.1",
    "@hashicorp/react-hashi-stack-menu": "1.1.0",
    "@hashicorp/react-head": "1.1.6",
    "@hashicorp/react-product-downloader": "4.1.1",
    "@hashicorp/react-search": "3.0.0",
    "@hashicorp/react-subnav": "7.1.0",
    "dotenv": "8.2.0",
    "gray-matter": "4.0.2",
    "next": "9.5.5",
    "next-mdx-remote": "2.1.1",
    "next-remote-watch": "0.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "dart-linkcheck": "2.0.15",
    "husky": "4.3.7",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "next-hashicorp precommit"
    }
  },
  "scripts": {
    "format": "next-hashicorp format",
    "generate-changelog": "node lib/generate-changelog.js",
    "generate:analytics": "next-hashicorp analytics",
    "generate:component": "next-hashicorp generate component",
    "generate:dato:types": "next-hashicorp codegen:datotypes",
    "linkcheck": "linkcheck https://docs.hashicorp.com/sentinel",
    "lint": "next-hashicorp lint",
    "start": "next-remote-watch ./content/**/*.mdx",
    "static": "next build && next export",
    "vercel-build": "npm run generate-changelog && next build"
  }
}
